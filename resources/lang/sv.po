msgid "Shared place"
msgstr "Delad plats"

#: SharedPlacesModule.php:326
msgid "Higher-level shared place"
msgstr "Delad plats på högre nivå"

#: SharedPlacesModule.php:327
msgid "Type of location"
msgstr "Typ av lokalisering"

#: SharedPlacesModule.php:328
msgid "Type of hierarchical relationship"
msgstr "Type av hierarkisk relation"

#: SharedPlacesModule.php:329
msgid "GOV id for type of location"
msgstr "GOV id för typ av lokalisering"

